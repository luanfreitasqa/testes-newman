		{
			"name": "Criação de Usuário",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 201\", function () {\r",
							"    pm.response.to.have.status(201); // Valida o Status Code\r",
							"});\r",
							"\r",
							"pm.test(\"Response is JSON\", function () {\r",
							"    pm.response.to.be.json; // Verifica se a resposta é JSON\r",
							"});\r",
							"\r",
							"pm.test(\"Response contains correct name and job\", function () {\r",
							"    var jsonData = pm.response.json(); // Obtém o JSON da resposta\r",
							"    pm.expect(jsonData.name).to.eql(\"Luan\"); // Valida o campo \"name\"\r",
							"    pm.expect(jsonData.job).to.eql(\"QA\"); // Valida o campo \"job\"\r",
							"});\r",
							"\r",
							"pm.test(\"Response has ID and createdAt\", function () {\r",
							"    var jsonData = pm.response.json(); // Obtém o JSON da resposta\r",
							"    pm.expect(jsonData).to.have.property('id'); // Verifica se o campo \"id\" existe\r",
							"    pm.expect(jsonData).to.have.property('createdAt'); // Verifica se o campo \"createdAt\" existe\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n  \"name\": \"Luan\",\r\n  \"job\": \"QA\"\r\n}",
